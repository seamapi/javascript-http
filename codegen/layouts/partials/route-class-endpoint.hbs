{{> route-class-endpoint-sig className=functionName}}
{
  return new SeamHttpRequest(this, {
    pathname: '{{path}}',
    method: '{{method}}',
    {{requestFormat}}: {{methodParamName}},
    responseKey: {{#if returnsVoid}}undefined{{else}}'{{responseKey}}'{{/if}},
    {{#if hasOptions}}options,{{/if}}
  })
}
